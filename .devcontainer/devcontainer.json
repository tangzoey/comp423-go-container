{
    "name": "Go Dev Container",
    "image": "mcr.microsoft.com/vscode/devcontainers/go:0-1.20",
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.Go"
            ],
            "settings": {
                "go.useLanguageServer": true
            }
        }
    },
    "postCreateCommand": "go version"
}

